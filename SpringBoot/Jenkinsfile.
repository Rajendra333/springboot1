pipeline{
  agent any 
  stages{
    stage('Clone'){
      steps{
        git 'https://github.com/Rajendra333/springboot1.git'
  }
 }
 stage('Build'){
   steps{
        bat 'mvn -f SpringBoot/pom.xml clean install'
        }
      }
  stage('test'){
    steps{
          bat 'mvn -f SpringBoot/pom.xml test'
          }
    }    
  }
}
